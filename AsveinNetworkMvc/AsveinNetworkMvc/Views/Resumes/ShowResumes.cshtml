@using AsveinNetworkMvc.Models;
@model List<Resumes>
@{
    Layout = null;
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
</div>
<div class="badge_details_wrap cover_detail">
    <div class="down_cover">

        <meta http-equiv="Content-Type" content="text/html; charset=gbk//IGNORE">
        <style>
            html, body, p, ul, table, tr, td {
                margin: 0;
                padding: 0
            }

            a {
                text-decoration: none
            }

            td {
                text-align: left
            }

            li {
                list-style: none
            }

            img {
                border: 0 none
            }

            body {
                font-family: 'Microsoft YaHei';
                font-size: 14px
            }

            div, span, a {
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                -o-box-sizing: border-box;
                -ms-box-sizing: border-box;
                box-sizing: border-box
            }

            table {
                border-collapse: collapse;
                border-spacing: 0;
                border: 0 none;
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                -ms-box-sizing: border-box;
                -o-box-sizing: border-box;
                box-sizing: border-box
            }

            .resume_clearfix {
                display: block;
                overflow: hidden;
                clear: both
            }

            .all_wrap {
                width: 693px
            }

            .at {
                white-space: nowrap;
                text-overflow: ellipsis;
                overflow: hidden
            }

            .page {
                width: 693px;
                margin: 0 auto;
                padding: 3px;
                background-color: #fff
            }

            .gray {
                color: #919191 !important
            }

            .l_green {
                color: #6d8194 !important
            }

            .blue {
                color: #4567af !important
            }

            .yellow {
                color: #fae001 !important
            }

            .yellow1 {
                color: #fae001 !important
            }

            .y_green {
                color: #a4b049 !important
            }

            .s_blue {
                color: #00a0e9 !important
            }

            .orange {
                color: #f39800 !important
            }

            .resume_box {
                border: 15px solid #efefef
            }

            .infomation_l {
                width: 266px;
                vertical-align: top;
                background-color: #2d4e5c
            }

            .top {
                margin: 26px 0px 18px 0px
            }

            .picture {
                text-align: center
            }

            .name {
                height: 40px;
                line-height: 40px;
                font-size: 18px;
                color: #fff;
                text-align: center
            }

            .posi {
                width: 265px;
                line-height: 14px;
                text-align: center;
                padding: 0 10px
            }

            .mes_boxl {
                margin: 10px 0 22px 0
            }

            .mes_l {
                margin-left: 20px
            }

            .title {
                height: 26px;
                font-weight: bold;
                color: #d8dcde
            }

            .border_b {
                margin-left: 20px;
                border-bottom: 1px solid #81959d
            }

            .per_list {
                line-height: 22px;
                font-size: 12px;
                color: #d8dcde
            }

            .title_iconl {
                *width: 12px;
                padding-right: 8px
            }

            .title_icon1 img {
                margin-top: 5px
            }

            .b_border {
                border-top: 1px solid #233c47
            }

            .dir {
                vertical-align: top;
                width: 100px;
            }

            .limit1 {
                width: 180px;
                text-align: left;
            }

            .wid {
                width: 225px
            }

            .limit2 {
                width: 204px
            }

            .con {
                position: relative;
                width: 176px;
                height: 16px;
                *height: 15px;
                top: 2px;
                border: 1px solid #b9b9b9
            }

            .rank {
                position: absolute;
                top: 0;
                left: 0;
                width: 70%;
                height: 16px;
                line-height: 16px;
                text-indent: 6px;
                top: 0px;
                left: 0px;
                background-color: #b9b9b9
            }

            .email {
                max-width: 146px;
                word-wrap: break-word
            }

            .infomation_r {
                width: 386px;
                vertical-align: top;
                padding-left: 12px;
                background-color: #efefef
            }

            .mes_r {
                margin-left: 26px;
                padding: 10px 0 16px 0
            }

            .title_icon {
                *width: 17px;
                padding-right: 8px;
                border: 0 none
            }

                .title_icon img {
                    margin-top: 5px
                }

            .title_txt {
                width: 340px;
                height: 28px;
                line-height: 28px;
                font-weight: bold;
                color: #2d4e5c;
                text-align: left;
                border-bottom: 1px solid #96a6ad
            }

            .title_txt1 {
                width: 235px;
                height: 27px;
                line-height: 27px;
                font-weight: bold;
                color: #d8dcde;
                text-align: left
            }

            .mes_detail {
                margin: 10px 0px 16px 0px
            }

            .mes_detail_a {
                margin-bottom: 10px
            }

            .mes_r .info_list {
                line-height: 22px;
                font-size: 12px;
                color: #000;
                overflow: hidden
            }

            .mes_r .limit {
                width: 290px
            }

            .mes_r .edu {
                width: 100px;
                vertical-align: top
            }

            .mes_r .edu_a {
                width: 100px;
                vertical-align: top
            }

            .mes_r .reu {
                max-width: 144px;
                vertical-align: top
            }

            .mes_r .reu_a {
                max-width: 190px;
                vertical-align: top;
                padding-right: 20px
            }

            .mes_r .in {
                line-height: 22px;
                font-size: 12px;
                color: #919191
            }

            .mes_r .exp {
                width: 180px
            }

            .mes_r .pofi {
                width: 106px;
                text-align: left;
                vertical-align: top
            }

            .mes_r .edu1 {
                width: 206px;
                margin-right: 20px
            }

            .mes_r .edu2 {
                width: 220px;
                padding-right: 20px
            }

                .mes_r .edu2 span {
                    display: block;
                    width: 220px;
                    white-space: nowrap;
                    overflow: hidden;
                    text-overflow: ellipsis
                }
        </style><div xmlns="" class="page">
            <div class="all_wrap">

                @foreach (var item in Model)
                {
                    @Html.ActionLink("修改", "UpdateResumes", new { @id = item.R_Id })
                    <table class="resume_box">
                        <tbody>
                            <tr>
                                <td class="infomation_l">
                                    <div class="top">
                                        <p class="picture"><img src="../images/@item.R_Picture" alt="简历照片" width="105" height="148"></p>
                                        <p class="name">@item.R_Name</p>
                                    </div>
                                    <table class="border_b">
                                        <tbody>
                                            <tr class="title">
                                                <td class="title_iconl" valign="middle"><img src="https://img02.51jobcdn.com/im/2017/resumetemplate/xsl/icon7_type1.png" width="12" height="13"></td>
                                                <td class="wid"><span>个人信息</span></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <div class="mes_boxl">
                                        <table class="mes_l">
                                            <tbody>
                                                <tr class="per_list">
                                                    <td class="dir"><span>工作经验：</span></td>
                                                    <td class="limit1"><span class="gray">@item.R_Experience 年以上</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <table class="mes_l">
                                            <tbody>
                                                <tr class="per_list">
                                                    <td class="dir"><span>性别：</span></td>
                                                    <td class="limit1">
                                                        <span class="gray">
                                                            男
                                                        </span>
                                                    </td>
                                                </tr>
                                                <tr class="per_list">
                                                    <td class="dir"><span>年龄：</span></td>
                                                    <td class="limit1"><span class="gray">@item.R_Age</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <table class="mes_l">
                                            <tbody>
                                                <tr class="per_list">
                                                    <td class="dir"><span>居住地：</span></td>
                                                    <td class="limit1"><span class="gray">@item.R_Address</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <table class="mes_l">
                                            <tbody>
                                                <tr class="per_list">
                                                    <td class="dir"><span>电话：</span></td>
                                                    <td class="limit2">
                                                        <span class="gray">
                                                            @item.R_Phone (手机)
                                                        </span>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <table class="mes_l">
                                            <tbody>
                                                <tr class="per_list">
                                                    <td class="dir"><span>E-mail：</span></td>
                                                    <td class="limit1"><span class="gray">@item.R_Email</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <table class="border_b">
                                        <tbody>
                                            <tr class="title">
                                                <td class="title_iconl" valign="middle"><img src="https://img03.51jobcdn.com/im/2017/resumetemplate/xsl/icon8_type1.png" width="12" height="13"></td>
                                                <td class="wid"><span>求职意向</span></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <div class="mes_boxl">
                                        <table class="mes_l">
                                            <tbody>
                                                <tr class="per_list">
                                                    <td class="dir"><span>从事职业：</span></td>
                                                    <td class="limit1"><span class="gray">@item.R_Occupation</span></td>
                                                </tr>
                                                <tr class="per_list">
                                                    <td><span>期望月薪：</span></td>
                                                    <td class="limit1"><span class="gray">@item.R_Salary</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <table class="border_b">
                                        <tbody>
                                            <tr class="title">
                                                <td class="title_iconl"><img src="https://img03.51jobcdn.com/im/2017/resumetemplate/xsl/icon9_type1.png" width="12" height="13"></td>
                                                <td class="wid"><span>语言能力</span></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <div class="mes_boxl">
                                        <table class="mes_l">
                                            <tbody>
                                                <tr class="per_list">
                                                    <td class="dir"><span>英语：</span></td>
                                                    <td class="limit2"><p class="con"><span class="rank">熟练</span></p></td>
                                                </tr>
                                                <tr class="per_list">
                                                    <td class="dir"><span>听说：</span></td>
                                                    <td class="limit2"><p class="con"><span class="rank">熟练</span></p></td>
                                                </tr>
                                                <tr class="per_list">
                                                    <td class="dir"><span>读写：</span></td>
                                                    <td class="limit2"><p class="con"><span class="rank">熟练</span></p></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </td>
                                <td class="infomation_r">
                                    <table class="per_title">
                                        <tbody>
                                            <tr>
                                                <td class="title_icon" valign="middle"><img src="https://img03.51jobcdn.com/im/2017/resumetemplate/xsl/icon1_type1.png" alt="教育经历" width="17" height="16"></td>
                                                <td width="340"><p class="title_txt">教育经历</p></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <div class="mes_r">
                                        <table>
                                            <tbody>
                                                <tr class="info_list">
                                                    <td class="edu"><span>学校：</span></td>
                                                    <td class="pofi"><span class="gray">@item.R_School</span></td>
                                                </tr>
                                                <tr class="info_list">
                                                    <td class="edu"><span>就读时间：</span></td>
                                                    <td class="pofi"><span class="gray">@item.R_Studyingtime</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <table>
                                            <tbody>
                                                <tr class="info_list">
                                                    <td class="edu"><span>专业：</span></td>
                                                    <td class="pofi"><span class="gray">@item.R_Major</span></td>
                                                </tr>
                                                <tr class="info_list">
                                                    <td class="edu"><span>学历学位：</span></td>
                                                    <td class="pofi"><span class="gray">@item.R_Degree</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <table class="per_title">
                                        <tbody>
                                            <tr>
                                                <td class="title_icon" valign="middle"><img src="https://img03.51jobcdn.com/im/2017/resumetemplate/xsl/icon2_type1.png" alt="最高学历" width="17" height="16"></td>
                                                <td width="340"><p class="title_txt">证书</p></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <div class="mes_r">
                                        <table>
                                            <tbody>
                                                <tr class="info_list">
                                                    <td class="edu"><span>计算机8级证书</span></td>
                                                </tr>
                                            </tbody>
                                        </table>

                                    </div>
                                    <table class="per_title">
                                        <tbody>
                                            <tr>
                                                <td class="title_icon" valign="middle"><img src="https://img03.51jobcdn.com/im/2017/resumetemplate/xsl/icon3_type1.png" alt="自我介绍" width="17" height="16"></td>
                                                <td width="340"><p class="title_txt">自我介绍</p></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <div class="mes_r"><p class="in">@item.R_Selfevaluation</p></div>
                                    <table class="per_title">
                                        <tbody>
                                            <tr>
                                                <td class="title_icon" valign="middle"><img src="https://img03.51jobcdn.com/im/2017/resumetemplate/xsl/icon4_type1.png" alt="工作经验" width="17" height="16"></td>
                                                <td width="340"><p class="title_txt">工作经验</p></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <div class="mes_r">
                                        <table>
                                            <tbody>
                                                <tr class="info_list"><td><span>工作内容：</span></td></tr>
                                                <tr class="in"><td>@item.R_WorkExperience</td></tr>
                                            </tbody>
                                        </table>
                                        <table><tbody><tr><td height="20"></td></tr></tbody></table>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                }
            </div>
        </div>
    </div>
</div>