@model AsveinNetworkMvc.Models.Company

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>InsertCompany</title>
</head>
<body>

    <h4>添加公司信息</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="InsertCompany">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="CName" class="control-label"></label>
                    <input asp-for="CName" class="form-control" />
                    <span asp-validation-for="CName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="CNameNature" class="control-label"></label>
                    <input asp-for="CNameNature" class="form-control" />
                    <span asp-validation-for="CNameNature" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Cnumber" class="control-label"></label>
                    <input asp-for="Cnumber" class="form-control" />
                    <span asp-validation-for="Cnumber" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="CompanyIndustry" class="control-label"></label>
                    <input asp-for="CompanyIndustry" class="form-control" />
                    <span asp-validation-for="CompanyIndustry" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="CompanyProfile" class="control-label"></label>
                    <input asp-for="CompanyProfile" class="form-control" />
                    <span asp-validation-for="CompanyProfile" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Companyposition" class="control-label"></label>
                    <input asp-for="Companyposition" class="form-control" />
                    <span asp-validation-for="Companyposition" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="添加" class="btn btn-default" />
                </div>
            </form>
        </div>
    </div>

</body>
</html>
